Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\YoutubeExplode.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\YouTubePlayer.deps.json"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\YouTubePlayer.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\YouTubePlayer.exe"; DestDir: "{app}"; Flags: ignoreversion
