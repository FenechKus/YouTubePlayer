Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\NAudio.Asio.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\NAudio.Core.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\NAudio.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "G:\source\C#\WPF\YouTubePlayer\YouTubePlayer\bin\Release\net8.0-windows\NAudio.Midi.dll"; DestDir: "{app}"; Flags: ignoreversion
